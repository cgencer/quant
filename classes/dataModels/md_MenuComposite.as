package classes.dataModels
{
	public class md_MenuComposite
	{
		private var level:uint;
		private var no0:uint;
		private var no1:uint;
		private var no2:uint;
		private var no3:uint;
		
		public function md_MenuComposite() {
			level = 0;
			no0 = 0;
			no1 = 0;
			no2 = 0;
			no3 = 0;
		}
		public function getLevel():uint {
			return level;
		}
		public function getNo0():uint {
			return no0;
		}
		public function getNo1():uint {
			return no1;
		}
		public function getNo2():uint {
			return no2;
		}
		public function getNo3():uint {
			return no3;
		}		
		public function setLevel(l:uint):void {
			level=l;
		}
		public function setNo0(n:uint):void {
			no0=n;
		}		
		public function setNo1(n:uint):void {
			no1=n;
		}		
		public function setNo2(n:uint):void {
			no2=n;
		}				
		public function setNo3(n:uint):void {
			no3=n;
		}				
		public function print():void
		{
			trace("level:"+level+" no0:"+no0+" no1:"+no1+" no2:"+no2+" no3:"+no3);
		}
	}
}