package classes.dataModels
{
	public class md_NewsElement
	{
		private var news_id:String;
		private var news_title:String;
		private var news_desc:String;
		private var news_image:String;
		private var news_date:String;
		private var news_color0:int;
		private var news_color1:int;

		public function md_NewsElement(nid:String, nt:String="", nd:String="", ni:String="", ndate:String="2008-01-01", ct:int=0xeeeeee, cd:int=0xaaaaaa) {
			news_id = nid;
			news_title = nt;
			news_desc = nd;
			news_image = ni;
			news_date = ndate;
			news_color0 = ct;
			news_color1 = cd;
		}

		public function getId():String {
			return news_id;
		}
		
		public function getTitle():String {
			return news_title;
		}

		public function getDesc():String {
			return news_desc;
		}

		public function getImage():String {
			return news_image;
		}

		public function getDate():String {
			return news_date;
		}		

		public function getColorT():int {
			return news_color0;
		}		

		public function getColorD():int {
			return news_color1;
		}		

	}
}